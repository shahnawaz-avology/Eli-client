{% comment %}
@font-face {
    font-family: 'Inter_24pt';
      	url({{'Inter_24pt-Bold.ttf' | file_url }}) format('truetype'),
        url({{'Inter_24pt-SemiBold.ttf' | file_url }}) format('truetype'),
        url({{'Inter_24pt-Regular.ttf' | file_url }}) format('truetype'),
        url({{'Inter_24pt-Medium.ttf' | file_url }}) format('truetype'),
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Channe';
        url({{'Channe.otf' | file_url }}) format('embedded-opentype'),
        url({{'Channe.ttf' | file_url }}) format('truetype'),
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}
@font-face {
    font-family: 'Redrains-Regular';
        url({{'Redrains-Regular.otf' | file_url }}) format('otf'),
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}
{% endcomment %}
